<?xml version="1.0" encoding="UTF-8"?>


<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<fx:root type="StackPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <GridPane minHeight="100.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <Label fx:id="label" alignment="TOP_LEFT" prefWidth="360.0" text="%dialog.save.saveFile" wrapText="true"/>
            <GridPane GridPane.rowIndex="1">
                <columnConstraints>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" percentWidth="33.0"/>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" percentWidth="34.0"
                                       prefWidth="100.0"/>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" percentWidth="33.0"
                                       prefWidth="100.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <children>
                    <JFXButton defaultButton="true" onAction="#save" styleClass="button-primary" text="%button.yes"/>
                    <JFXButton onAction="#doNotSave" styleClass="button-secondary" text="%button.no"
                               GridPane.columnIndex="1"/>
                    <JFXButton onAction="#cancel" styleClass="button-secondary" text="%button.cancel"
                               GridPane.columnIndex="2"/>
                </children>
            </GridPane>
            <opaqueInsets>
                <Insets left="20.0"/>
            </opaqueInsets>
            <StackPane.margin>
                <Insets/>
            </StackPane.margin>
        </GridPane>
    </children>
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <padding>
        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
    </padding>
</fx:root>
